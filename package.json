{
  "name": "dooform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:web": "pnpm --filter @dooform/web dev",
    "dev:salespage": "pnpm --filter @dooform/salespage dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @dooform/web build",
    "build:salespage": "pnpm --filter @dooform/salespage build",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@10.18.0"
}
